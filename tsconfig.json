{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "@/config": [
        "config"
      ],
      "@/replies": [
        "replies"
      ],
      "@/values": [
        "values"
      ],
      "@/repositories": [
        "repositories"
      ],
      "@/logger": [
        "logger"
      ],
      "@/database": [
        "database"
      ],
      "@/middlewares": [
        "middlewares"
      ]
    },
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}