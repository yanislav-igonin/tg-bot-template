  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/validator": "^13.11.6",
    "@typescript-eslint/parser": "^5.55.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.53.0",
    "eslint-config-canonical": "^40.0.9",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.3",
